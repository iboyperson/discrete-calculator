{
  "name": "discrete-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write src",
    "index": "cti ./src/components/*/",
    "build:grammar:truthtable": "nearleyc src/util/parser/truthtable/grammar.ne -o src/util/parser/truthtable/grammar.ts",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@matejmazur/react-katex": "^3.1.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "asciimath2tex": "^1.1.0",
    "clsx": "^1.1.1",
    "katex": "^0.12.0",
    "nearley": "^2.20.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-math-view": "^1.2.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.15",
    "@types/nearley": "^2.11.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-table": "^7.0.28",
    "create-ts-index": "^1.13.6",
    "husky": "^4",
    "lint-staged": "^10.5.4",
    "mathlive": "^0.59.0",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.2",
    "typescript": "^4.1.5"
  }
}
